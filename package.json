{
  "name": "@zaius/app-sdk",
  "version": "0.17.0",
  "description": "Zaius App SDK and interfaces",
  "repository": "https://github.com/ZaiusInc/app-sdk",
  "license": "UNLICENSED",
  "author": "Zaius, Inc",
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {},
  "scripts": {
    "build": "yarn update-schema && tsc",
    "build-watch": "tsc -w",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json",
    "test": "jest",
    "update-schema": "typescript-json-schema --required --noExtraProps tsconfig.json AppManifest > ./src/app/types/AppManifest.schema.json && typescript-json-schema --required --noExtraProps tsconfig.json SchemaObject > ./src/app/types/SchemaObject.schema.json",
    "generate-docs": "typedoc ./src/index.ts"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@zaius/app-forms-schema": "^0.3.5",
    "ajv": "^6.10.0",
    "deep-freeze": "^0.0.1",
    "email-validator": "^2.0.4",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "remark": "^11.0.1",
    "remark-validate-links": "^9.0.1",
    "url-regex": "^5.0.0",
    "csv-parser": "^2.3.0",
    "node-fetch": "^2.6.0",
    "object-hash": "^1.3.1"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^12.6.9",
    "@types/node-fetch": "^2.5.0",
    "@types/object-hash": "^1.3.0",
    "@types/nock": "^10.0.3",
    "@zaius/node-sdk": "^0.7.1",
    "@zaiusinc/tslint-presets": "^1.0.1",
    "jest": "^24.5.0",
    "mock-fs": "^4.8.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.14.0",
    "typedoc": "^0.16.2",
    "typescript": "^3.7.4",
    "typescript-json-schema": "^0.42.0",
    "nock": "^10.0.6"
  },
  "peerDependencies": {
    "@zaius/node-sdk": "^0.7.1"
  },
  "publishConfig": {
    "registry": "http://npm-registry.prod.zaius"
  }
}
