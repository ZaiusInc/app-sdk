{
  "name": "@zaiusinc/app-sdk",
  "version": "1.2.6",
  "description": "Optimizely Connect Platform App SDK and interfaces",
  "repository": "https://github.com/ZaiusInc/app-sdk",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "author": "Optimizely",
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {},
  "scripts": {
    "build": "yarn update-schema && tsc",
    "build-watch": "tsc -w",
    "lint": "npx eslint 'src/**/*.ts'",
    "test": "jest -i",
    "update-schema": "typescript-json-schema --required --noExtraProps tsconfig.json AppManifest > ./src/app/types/AppManifest.schema.json && typescript-json-schema --required --noExtraProps src/app/types/SchemaObject.ts SchemaObject > ./src/app/types/SchemaObject.schema.json",
    "prepare-readme-export": "node scripts/prepare_readme_export.js",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown ./src/index.ts && yarn prepare-readme-export"
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "@zaiusinc/app-forms-schema": "^1.0.1",
    "ajv": "^8.11.0",
    "cron-expression-validator": "^1.0.20",
    "csv-parser": "^3.0.0",
    "deep-freeze": "^0.0.1",
    "email-validator": "^2.0.4",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "object-hash": "^3.0.0",
    "remark": "^11.0.1",
    "remark-validate-links": "^9.0.1",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.2",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.184",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.7.13",
    "@types/node-fetch": "^2.6.2",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.35.1",
    "@typescript-eslint/parser": "^5.0.0",
    "@zaiusinc/eslint-config-presets": "^1.0.0",
    "@zaiusinc/node-sdk": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.1",
    "jest": "^28.1.3",
    "mock-fs": "^5.1.4",
    "nock": "^13.2.9",
    "prettier": "^2.8.7",
    "slugify": "^1.6.6",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^5.14.0",
    "typedoc": "^0.24.0",
    "typedoc-plugin-markdown": "^4.0.0-next.4",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.54.0"
  },
  "peerDependencies": {
    "@zaiusinc/node-sdk": "^1.0.0"
  }
}
