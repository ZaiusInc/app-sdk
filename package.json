{
  "name": "@zaius/app-sdk",
  "version": "1.0.0-dev.1",
  "description": "Zaius App SDK and interfaces",
  "repository": "https://github.com/ZaiusInc/app-sdk",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "author": "Zaius, Inc",
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {},
  "scripts": {
    "build": "yarn update-schema && tsc",
    "build-watch": "tsc -w",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json",
    "test": "jest -i",
    "update-schema": "typescript-json-schema --required --noExtraProps tsconfig.json AppManifest > ./src/app/types/AppManifest.schema.json && typescript-json-schema --required --noExtraProps src/app/types/SchemaObject.ts SchemaObject > ./src/app/types/SchemaObject.schema.json",
    "generate-docs": "typedoc ./src/index.ts"
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "@zaiusinc/app-forms-schema": "1.0.0-dev.1",
    "ajv": "^8.11.0",
    "csv-parser": "^3.0.0",
    "deep-freeze": "^0.0.1",
    "email-validator": "^2.0.4",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "remark": "^11.0.1",
    "remark-validate-links": "^9.0.1",
    "url-regex": "^5.0.0",
    "node-fetch": "^2.6.7",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.2",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.184",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.7.13",
    "@types/node-fetch": "^2.6.2",
    "@types/object-hash": "^2.2.1",
    "@zaiusinc/node-sdk": "1.0.0-dev.1",
    "@zaiusinc/tslint-presets": "^1.0.1",
    "jest": "^29.0.0",
    "mock-fs": "^5.1.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "nock": "^13.2.9",
    "tslint": "^5.14.0",
    "typedoc": "^0.23.11",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.54.0"
  },
  "peerDependencies": {
    "@zaiusinc/node-sdk": "1.0.0-dev.1"
  }
}
